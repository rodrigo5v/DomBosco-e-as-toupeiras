    <ImageView
        android:id="@+id/ImgSB6"
        android:onClick="ChangeImg6"
        android:layout_width="92dp"
        android:layout_height="42dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="76dp"
